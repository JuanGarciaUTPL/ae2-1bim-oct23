// Clase Vehículo
public class Vehiculo {

    private String marca;
    private String modelo;
    private String color;
    private String matricula;

    public Vehiculo(String marca, String modelo, String color, String matricula) {
        this.marca = marca;
        this.modelo = modelo;
        this.color = color;
        this.matricula = matricula;
    }

    @Override
    public String toString() {
        return "Vehículo de marca " + marca + ", modelo " + modelo + ", color " + color + " y matrícula " + matricula;
    }
}

// Clase Empresa
public class Empresa {

    private String nombre;
    private String cif;
    private String actividad;

    public Empresa(String nombre, String cif, String actividad) {
        this.nombre = nombre;
        this.cif = cif;
        this.actividad = actividad;
    }

    @Override
    public String toString() {
        return "Empresa de nombre " + nombre + ", cif " + cif + " y actividad " + actividad;
    }
}

// Creación dinámica de objetos
public static Vehiculo crearVehiculo(String marca, String modelo, String color, String matricula) {
    return new Vehiculo(marca, modelo, color, matricula);
}

public static Empresa crearEmpresa(String nombre, String cif, String actividad) {
    return new Empresa(nombre, cif, actividad);
}